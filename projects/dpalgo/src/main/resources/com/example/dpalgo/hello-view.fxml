<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="676.0" prefWidth="920.0"
      style="-fx-background-image: url('/com/example/dpalgo/img.png');"
      xmlns="http://javafx.com/javafx/18"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.dpalgo.HelloController">

   <children>
      <Label layoutX="401.0" layoutY="26.0" prefHeight="35.0" prefWidth="119.0" text="Let's Travel !" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>

      <TextArea fx:id="dpTableTA" editable="false" layoutX="14.0" layoutY="127.0" prefHeight="230.0" prefWidth="324.0" style="-fx-background-color: lightgrey;" />
      <Button fx:id="browsebtn" layoutX="587.0" layoutY="61.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Browse">
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.55" radius="0.5">
               <stops>
                  <Stop offset="0" color="#b1a2a2"/>
                  <Stop offset="0.61" color="#0119ef"/>
                  <Stop offset="1.0" color="#309499"/>
               </stops>
            </RadialGradient>
         </textFill>
      </Button>

      <Button fx:id="travelbtn" layoutX="251.0" layoutY="61.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="83.0" text="Start Trip">
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.38" radius="0.5">
               <stops>
                  <Stop offset="0" color="#b1a2a2"/>
                  <Stop offset="0.61" color="#0119ef"/>
                  <Stop offset="1.0" color="#30abc0"/>
               </stops>
            </RadialGradient>
         </textFill>
      </Button>

      <TextField fx:id="filename" editable="false" layoutX="355.0" layoutY="61.0" prefHeight="35.0" prefWidth="211.0" />
      <Label fx:id="lbl2" layoutX="24.0" layoutY="96.0" prefHeight="35.0" prefWidth="93.0" text="DP table:" textFill="#56ccd4" visible="false">
         <font><Font name="Berlin Sans FB" size="17.0"/></font>
      </Label>

      <Label fx:id="lbl3" layoutX="14.0" layoutY="357.0" prefHeight="35.0" prefWidth="93.0" text="Path table:" textFill="#56ccd4" visible="false">
         <font><Font name="Berlin Sans FB" size="17.0"/></font>
      </Label>

      <TextArea fx:id="pathTableTA" editable="false" layoutX="14.0" layoutY="395.0" prefHeight="273.0" prefWidth="324.0" style="-fx-background-color: lightgrey;" />
      <Label fx:id="lbl1" layoutX="524.0" layoutY="143.0" prefHeight="42.0" prefWidth="127.0" text="Shortest Path" textFill="#ffa98d" visible="false">
         <font><Font name="Berlin Sans FB" size="17.0"/></font>
      </Label>

      <Label fx:id="costl" layoutX="365.0" layoutY="185.0" prefHeight="42.0" prefWidth="48.0" text="Cost:" textFill="#56ccd4" visible="false">
         <font><Font name="Berlin Sans FB" size="17.0"/></font>
      </Label>

      <Label fx:id="pathl" layoutX="365.0" layoutY="274.0" prefHeight="42.0" prefWidth="127.0" text="Path:" textFill="#56ccd4" visible="false">
         <font><Font name="Berlin Sans FB" size="17.0"/></font>
      </Label>

      <Label fx:id="costlbl" layoutX="419.0" layoutY="185.0" prefHeight="42.0" prefWidth="127.0" text="cost here" textFill="#ffa98d">
         <font><Font name="Berlin Sans FB" size="17.0"/></font>
      </Label>

      <Label fx:id="pathlbl" layoutX="419.0" layoutY="236.0" prefHeight="118.0" prefWidth="490.0" text="Path here" textFill="#ffa98d">
         <font><Font name="Berlin Sans FB" size="17.0"/></font>
      </Label>

      <TextArea fx:id="alternTableTA" editable="false" layoutX="396.0" layoutY="395.0" prefHeight="273.0" prefWidth="341.0" style="-fx-background-color: lightgrey;" />

      <Label fx:id="lbl4" layoutX="401.0" layoutY="353.0" prefHeight="42.0" prefWidth="278.0" text="Alternative routes and their costs:" textFill="#c7fcff" visible="false">
         <font><Font name="Berlin Sans FB" size="17.0"/></font>
      </Label>
   </children>
</Pane>
